<%- include('../templates/pheader.ejs') %>
<section class="p1">
  <% if (data) { %>
  <div class="container-fluid product_view">
    <table class="table table-striped table-bordered">
      <thead>
        <td>ID</td>
        <td>PO NO.</td>
        <td>PRODUCT CODE</td>
        <td>NAME</td>
        <td>DESCRIPTION</td>
        <td>QUANTITY</td>
        <td>TOTAL COST</td>
        <td>DATE OF P.O.</td>
        <td>PARTY NAME</td>
      </thead>
      <% data.forEach(function(dd){ %>
      <tr>
        <td><%= dd.id %></td>
        <td><%= dd.r_code %></td>
        <td><%= dd.p_code %></td>

        <td><%= dd.r_name %></td>
        <td><%= dd.r_description %></td>
        
        <td><%= dd.r_qty %></td>
        <td><%= dd.r_cost %></td>
         
        <td><%= dd.r_issue_date.toLocaleDateString() %></td>
        <td><%= dd.r_party_name %></td>
      </tr>

      <% }) %>
    </table>
  </div>

  <% } %>
</section>

<%- include('../templates/footer.ejs') %>