<script>
    $(document).ready(function(){



        $("#clear").on("click", function(){
             $(".form-group #p_id").val("")
             $(".form-group #p_withdraw_date").val("")
             $(".form-group #p_name").val("")
             $(".form-group #p_description").val("")
             $(".form-group #p_qty").val("")
        })

       $("#clearq").on("click", function(){
             $(".form-group #p_id").val("")
             $(".form-group #p_withdraw_date").val("")
             $(".form-group #p_name").val("")
             $(".form-group #p_description").val("")
             $(".form-group #p_qty").val("")
        })


        $("#find").click(function(e){
    
             let id = $( "#p_id").val()
             e.preventDefault();
             $.ajax({type: "POST",
                url: "/store/find/"+id,
                success:function(result){
                $(" #p_name").val(result.p_name)
                $(" #p_description").val(result.p_description)
                 },
                 error:function(result)
                 {
                alert('error');
                }
              });
        });
    


        $("#findq").click(function(e){
    
        let id = $( "#update_p #p_id").val()
    
          e.preventDefault();
            $.ajax({type: "POST",
                url: "/store/find/"+id,
                success:function(result){
                $(" #p_name").val(result.p_name)
                $(" #p_description").val(result.p_description)
                },
               error:function(result)
                {
                alert('error');
               }
              });
            });



         $("#findpodata").click(function(e){
    
         
    
          e.preventDefault();
            $.ajax({type: "POST",
                url: "/purchase/find/"+id,
